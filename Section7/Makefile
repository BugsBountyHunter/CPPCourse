.PHONY: clean All

All:
	@echo "----------Building project:[ ChallengeSolution - Debug ]----------"
	@cd "ChallengeSolution" && "$(MAKE)" -f  "ChallengeSolution.mk"
clean:
	@echo "----------Cleaning project:[ ChallengeSolution - Debug ]----------"
	@cd "ChallengeSolution" && "$(MAKE)" -f  "ChallengeSolution.mk" clean
